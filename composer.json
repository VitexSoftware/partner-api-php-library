{
    "name": "slevomat/partner-api-php-library",
    "description": "PHP library for the Slevomat.cz/Zlavomat.sk voucher API for partners",
    "keywords": ["slevomat", "zlavomat", "voucher", "api"],
    "homepage": "https://github.com/slevomat/partner-api-php-library",
    "license": ["proprietary"],
    "authors": [
        {
            "name": "Slevomat.cz, s.r.o.",
            "homepage": "http://www.slevomat.cz"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Slevomat\\": "Slevomat",
            "Slevomat\\Client\\": "Slevomat/Client",
            "Slevomat\\Client\\Response\\": "Slevomat/Client/Response"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Slevomat\\": "test/Slevomat",
            "Test\\Slevomat\\Client\\": "test/Slevomat/Client",
            "Test\\Slevomat\\Client\\Response\\": "test/Slevomat/Client/Response"
        }
    }
}
